<h1>New school_property</h1>

<% form_for(@school_property) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :school_id %><br />
    <%= f.text_field :school_id %>
  </p>
  <p>
    <%= f.label :property_id %><br />
    <%= f.text_field :property_id %>
  </p>
  <p>
    <%= f.label :transport_id %><br />
    <%= f.text_field :transport_id %>
  </p>
  <p>
    <%= f.label :minutes %><br />
    <%= f.text_field :minutes %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', school_properties_path %>